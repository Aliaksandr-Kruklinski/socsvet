@model MvcUI.Models.Results

<div class="stroke">




    @for (int i = 0; i < Model.Subjects.Count(); i++)
    {
        <div class="result">
            @Html.HideImage(@Model.Subjects[i].Id)
            <label> @Model.Subjects[i].Name</label>
            <div id="@Model.Subjects[i].Id">
                @foreach (var test in @Model.Tests.Where(t => t.SubjectId == @Model.Subjects[i].Id))
                {
                    <label>@test.Name : @Model.Result.Where(r => r.TestId == test.Id).First().Description</label>
                    <br/>
                }
            </div>
        </div>
        <br />
    }


</div>