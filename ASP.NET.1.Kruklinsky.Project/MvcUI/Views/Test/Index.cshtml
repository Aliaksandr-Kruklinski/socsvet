@model IEnumerable<MvcUI.Models.SubjectEditor>

@section Links
{
    @Styles.Render("~/Content/home/css")
}
@section scripts
{
    @Scripts.Render("~/Content/admin/js")
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jqueryval")
}
@{
    ViewBag.Title = "Test";
}

<div>
    <div class="stroke">
        @foreach (var subject in Model)
        {
            <div class="subject">
                @Html.HideImage(subject.Subject.Id)
                <label>@subject.Subject.Name</label>
                <div id="@subject.Subject.Id">
                    @foreach (var test in subject.Tests)
                    {
                        @Html.ActionLink(test.Name, "Test", "Test", new { testId = test.Id }, null)
                        <br />
                    }
                </div>
            </div>
        }
    </div>
</div>